{
  "name": "@chatui/ui",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/ui.css",
    "./dist/ui.css": "./dist/ui.css",
    "./forms": {
      "types": "./dist/app/components/ui/forms/index.d.ts",
      "import": "./dist/forms.js"
    },
    "./chat": {
      "types": "./dist/app/components/ui/chat/index.d.ts",
      "import": "./dist/chat.js"
    },
    "./layout": {
      "types": "./dist/app/components/ui/layout/index.d.ts",
      "import": "./dist/layout.js"
    },
    "./templates": {
      "types": "./dist/templates/index.d.ts",
      "import": "./dist/templates.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/ui.css"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:components": "test-storybook",
    "test:a11y": "test-storybook --coverage",
    "test:visual": "chromatic --exit-zero-on-changes",
    "analyze": "echo 'Bundle analysis available in Storybook addon-webpack-stats-v2'",
    "clean": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@chatui/tokens": "workspace:*",
