{
  "name": "@astudio/effects",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles/animations.css": "./src/styles/animations.css",
    "./styles/filters.css": "./src/styles/filters.css",
    "./styles/base.css": "./src/styles/base.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": [
