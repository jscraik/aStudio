{
  "name": "@astudio/ui",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.js"
    },
    "./styles.css": "./src/styles.css",
    "./base": {
      "types": "./dist/base.d.ts",
      "import": "./dist/base.js"
    },
    "./data-display": {
      "types": "./dist/data-display.d.ts",
      "import": "./dist/data-display.js"
    },
    "./feedback": {
      "types": "./dist/feedback.d.ts",
      "import": "./dist/feedback.js"
    },
    "./forms": {
      "types": "./dist/forms.d.ts",
      "import": "./dist/forms.js"
    },
    "./chat": {
      "types": "./dist/chat.d.ts",
      "import": "./dist/chat.js"
    },
    "./layout": {
      "types": "./dist/layout.d.ts",
      "import": "./dist/layout.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.js"
    },
    "./overlays": {
      "types": "./dist/overlays.d.ts",
      "import": "./dist/overlays.js"
    },
    "./modals": {
      "types": "./dist/modals.d.ts",
      "import": "./dist/modals.js"
    },
    "./settings": {
      "types": "./dist/settings.d.ts",
      "import": "./dist/settings.js"
    },
    "./showcase": {
      "types": "./dist/showcase.d.ts",
      "import": "./dist/showcase.js"
    },
    "./templates": {
      "types": "./dist/templates.d.ts",
      "import": "./dist/templates.js"
    },
    "./experimental": {
      "types": "./dist/experimental.d.ts",
      "import": "./dist/experimental.js"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "./src/styles.css"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@astudio/tokens": "workspace:*",
    "@chatui/ui": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
