{
  "name": "@astudio/tokens",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tokens.css": "./src/tokens.css",
    "./foundations.css": "./src/foundations.css",
    "./icon-picker-theme.css": "./src/icon-picker-theme.css",
    "./tailwind.preset": {
      "types": "./dist/tailwind.preset.d.ts",
      "import": "./dist/tailwind.preset.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": [
    "./src/foundations.css",
    "./src/tokens.css",
    "./src/icon-picker-theme.css"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chatui/tokens": "workspace:*"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.12"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
